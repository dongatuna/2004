rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /courses/{courseId}  {
      allow read, write      
    }
    match /students/{studentId} {
      allow read, write      
    }
    match /jobs/{jobId} {
      allow read, write    
    }
    match /reservations/{reservationId} {
      allow read, write    
    }
    match /prospects/{prospectId} {
      allow read, write    
    }
  }
}